{
  "name": "Screegle",
  "author": "Laurent Denoue",
  "manifest_version": 2,
  "version": "1.0.0",
  "description": "Screegle Clean Screen Sharing",
  "background": {"scripts": ["screegle-background.js"]},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "permissions": ["nativeMessaging","tabs","<all_urls>"],
  "browser_action": {
	},
	"icons": {
   "128": "screegle.png"
  },
  "web_accessible_resources": [
    "*"
  ]
}
